            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <!--***ACTIVITIES*** -->
        <activity android:name=".activity.ActivitySlideMenu"/>
        <activity android:name=".activity.ActivityFileBrowser"/>
        <activity android:name=".activity.ActivityReminder"/>
        <activity android:name=".activity.ActivityBookReader"/>
        <activity android:name=".activity.ActivityFireBase"/>

        <activity android:name=".activity.ActivityDrawing"/>
        <activity android:name=".activity.ActivityCreateIcon"/>
        <activity android:name=".activity.ActivityUIDesign"/>
        <activity android:name=".activity.ActivityMusic"/>
        <activity android:name=".activity.Activity3DDraw"/>


    </application>

</manifest>