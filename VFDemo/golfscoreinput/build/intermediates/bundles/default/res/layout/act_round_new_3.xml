<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Button
        android:layout_width="match_parent"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="15dp"
        android:id="@+id/btn_new_round_more_group3"
        android:text="@string/btn_new_round_hide_group_3"
        android:textAllCaps="false"
        android:textColor="@color/gray3"
        android:background="@drawable/golf_detail_item_center_selector"
        android:layout_height="40dp" />
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="gone"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp"
        android:id="@+id/group3">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginBottom="5dp"
            android:weightSum="3">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="2">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/tv_setting_add_player_new_ground"
                    android:textColor="@color/gray3"
                    android:id="@+id/textView16"
                    style="@style/background_text_new_group_360_480" />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="fill_parent"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="@string/stroke_handicap_signature"
                    android:layout_gravity="center"
                    android:textColor="@color/gray3"
                    android:id="@+id/textView14"
                    style="@style/background_text_new_group_360_480" />

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:background="@color/gray"
                    android:layout_height="2dp"></LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="5dp"
            android:weightSum="3">

            <RelativeLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:gravity="center_vertical"
                android:layout_height="35dp"
                android:id="@+id/player1Layout"
                android:background="@color/gray3">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:orientation="vertical"
                    android:background="@color/gray8"
                    android:layout_marginLeft="1.5dp"
                    android:layout_marginRight="1.5dp"
                    android:paddingBottom="1dp"
                    android:paddingTop="1dp"
                    android:layout_height="32dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="30dp"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Medium Text"
                        android:gravity="center_vertical"
                        android:layout_marginLeft="10dp"
                        android:textColor="@color/gray"
                        android:id="@+id/player1"
                        style="@style/background_text_new_group_360_480" />
                </LinearLayout>


            </RelativeLayout>

            <RelativeLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:id="@+id/curHandicap1Layout"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text=""
                    android:layout_centerInParent="true"
                    android:textColor="@color/black"
                    android:id="@+id/tvCurPlayerHandicap"
                    style="@style/background_text_new_group_360_480" />

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="45dp"
                    android:background="@color/gray3"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_centerInParent="true"

                    android:layout_below="@+id/tvCurPlayerHandicap"
                    android:layout_marginTop="1dp"
                    android:layout_height="1dp"></LinearLayout>
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="5dp"
            android:weightSum="3">

            <RelativeLayout
                android:id="@+id/player2Layout"
                android:orientation="horizontal"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:gravity="center_vertical"
                android:layout_height="35dp"
                android:background="@drawable/golf_detail_item_center_selector">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:hint="@string/select_player_name"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/black"
                    android:id="@+id/player2"
                    style="@style/background_text_new_group_360_480" />
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/clear_player2"
                    android:layout_alignParentRight="true">
                    <Button
                        android:id="@+id/clear_player2_btn"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_margin="7dp"
                        android:background="@drawable/btn_clear_select_player" />
                </LinearLayout>

            </RelativeLayout>

            <RelativeLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:id="@+id/handicap2Layout"
                android:layout_gravity="center"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text=""
                    android:layout_centerInParent="true"
                    android:textColor="@color/black"
                    android:id="@+id/tvPlayer2Handicap"
                    style="@style/background_text_new_group_360_480" />

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="45dp"
                    android:background="@color/gray3"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_centerInParent="true"
                    android:layout_below="@+id/tvPlayer2Handicap"
                    android:layout_marginTop="1dp"
                    android:layout_height="1dp"></LinearLayout>
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:weightSum="3">

            <RelativeLayout
                android:id="@+id/player3Layout"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:gravity="center_vertical"
                android:layout_height="35dp"
                android:background="@drawable/golf_detail_item_center_selector">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:hint="@string/select_player_name"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/black"
                    android:id="@+id/player3"
                    style="@style/background_text_new_group_360_480" />
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/clear_player3"
                    android:layout_alignParentRight="true">
                    <Button
                        android:id="@+id/clear_player3_btn"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_margin="7dp"
                        android:background="@drawable/btn_clear_select_player" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:id="@+id/handicap3Layout"
                android:layout_gravity="center"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text=""
                    android:layout_centerInParent="true"
                    android:textColor="@color/black"
                    android:id="@+id/tvPlayer3Handicap"
                    style="@style/background_text_new_group_360_480" />

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="45dp"
                    android:background="@color/gray3"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_centerInParent="true"
                    android:layout_below="@+id/tvPlayer3Handicap"
                    android:layout_marginTop="1dp"
                    android:layout_height="1dp"></LinearLayout>
            </RelativeLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="5dp"
            android:weightSum="3">

            <RelativeLayout
                android:id="@+id/player4Layout"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:gravity="center_vertical"
                android:layout_height="35dp"
                android:background="@drawable/golf_detail_item_center_selector">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:hint="@string/select_player_name"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:textColor="@color/black"
                    android:id="@+id/player4"
                    style="@style/background_text_new_group_360_480" />
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:id="@+id/clear_player4"
                    android:layout_alignParentRight="true">
                    <Button
                        android:id="@+id/clear_player4_btn"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_margin="7dp"
                        android:background="@drawable/btn_clear_select_player" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="30dp"
                android:id="@+id/handicap4Layout"
                android:layout_gravity="center"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text=""
                    android:layout_centerInParent="true"
                    android:textColor="@color/black"
                    android:id="@+id/tvPlayer4Handicap"
                    style="@style/background_text_new_group_360_480" />

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="45dp"
                    android:background="@color/gray3"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_centerInParent="true"
                    android:layout_below="@+id/tvPlayer4Handicap"
                    android:layout_marginTop="1dp"
                    android:layout_height="1dp"></LinearLayout>
            </RelativeLayout>
        </LinearLayout>

    </LinearLayout>
</LinearLayout>